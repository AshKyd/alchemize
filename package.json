{
  "name": "alchemize",
  "displayName": "Alchemize",
  "version": "3.0.0",
  "description": "Minify & pretty print source code with ease.",
  "main": "src/scripts/index.js",
  "type": "module",
  "private": "true",
  "repository": "github:AshKyd/alchemize",
  "bugs": {
    "url": "https://github.com/AshKyd/alchemize/issues"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build-app": "VITE_COPYRIGHT_YEAR=$(date +%Y) vite build && node docker/insertPreloads.js",
    "build-web": "rm -rf dist-www;mkdir dist-www;npm run build-app;cp web/* dist-www/;cp -r dist dist-www/app;find dist-www -type f -exec brotli --best --verbose --keep {} \\;",
    "build": "npm run build-app"
  },
  "author": {
    "name": "Ash Kyd",
    "email": "ash@kyd.com.au",
    "url": "https://ashk.au/"
  },
  "license": "ISC",
  "dependencies": {
    "@preact/signals": "^2.2.1",
    "@prettier/plugin-xml": "^3.4.2",
    "@rollup/plugin-inject": "^5.0.5",
    "buffer": "^6.0.3",
    "csso": "^5.0.5",
    "html-minifier-terser": "^7.2.0",
    "https-browserify": "^1.0.0",
    "js-beautify": "^1.15.4",
    "monaco-editor": "^0.52.2",
    "monaco-themes": "^0.4.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "preact": "^10.22.1",
    "prettier": "^3.6.2",
    "pretty-data": "github:AshKyd/pretty-data",
    "shade-blend-color": "^1.0.0",
    "stream-http": "^3.2.0",
    "terser": "^5.43.1",
    "util": "^0.12.5",
    "workiq": "^0.0.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.9.0",
    "vite": "^5.3.3"
  }
}
